@model BlogDashBoard

    <h2> Tb User</h2><hr />
<table class="table table-dark">
    <thead>
        <tr>
            <th>Username</th>
            <th>Password</th>
            <th></th>
            <th></th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.user)
        {
        <tr>
            <td>@item.Username</td>
            <td>@item.Password</td>
            <td> <button style="background-color:green;">Edit</button></td>
            <td>
                <button style="background-color: aqua">Details</button>
            </td>
            <td>
                <button style="background-color: red;">Hapus</button>
            </td>
        </tr>
        }
    </tbody>
</table>
